{
  "name": "checkin-server",
  "version": "1.0.0",
  "description": "API for checkin",
  "main": "lib/server.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "docker:build": "docker build -t checkin-server .",
    "docker:run": "docker run -d --name checkin-server -v \"$PWD\":/usr/local/app --net=checkin-net -p 8000:8000 -p 9222:9222 checkin-server",
    "docker:shutdown": "docker container stop checkin-server && docker container rm checkin-server",
    "lint": "tslint -e node_modules -p tsconfig.json -t stylish",
    "lint:fix": "tslint -e node_modules -p tsconfig.json --fix",
    "start": "node lib/server",
    "test": "jest"
  },
  "author": "Gabriel Foust",
  "license": "ISC",
  "dependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/mongoose": "^5.2.5",
    "@types/morgan": "^1.7.35",
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "express-ws": "^4.0.0",
    "mongoose": "^5.2.8",
    "morgan": "^1.9.0"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/express-ws": "^3.0.0",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1"
  }
}
